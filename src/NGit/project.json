{
  "version": "1.0.0-*",

  "dependencies": {
    "NSch": "1.0.0"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      },
      "buildOptions": {
        "define": [ "NETCORE" ]
      }
    },
    "net451": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ],
      "frameworkAssemblies": {
        "System.Web": "4.0.0"
      }
    }
  },

  "buildOptions": {
    "preserveCompilationContext": true,
    "define": [],
    "xmlDoc": true,
    "nowarn": [ "CS1591" ],
    "embed": [ "resources/*.*" ]
  }
}
